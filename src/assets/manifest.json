{  
   "manifest_version":2,
   "name":"Test Extension",
   "version":"1.0.0",
   "permissions":[  
      "tabs",
      "activeTab"
   ],
   "background":{  
      "page":"index.html#/home/my",
      "persistent":false
   },
   "browser_action":{  
      "default_title":"Open Popup!",
      "default_popup":"index.html#/home/my"
   },
   "icons":{  
      "19":"assets/Icon-19.png",
      "38":"assets/Icon-38.png"
   },
   "content_scripts":[  
      {  
         "matches":[  
            "*://*/*"
         ],
         "js":[  
            "scripts.bundle.js"
         ]
      }
   ]
}